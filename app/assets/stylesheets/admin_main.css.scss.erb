.group_cell{
  float: left;
  width: 290px;
  padding-right: 25px;
}
.group_cell_opts{
    float: right;
}
.destroy_group_provider{
    display: none;
}
#generate_pass{
    background-color: #e00000;
    color: #fff;
    padding: 3px;
    cursor: pointer;
    left: 160px;
    top: -60px;
    position: relative;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
}

.admin_current_user{
    padding: 5px;
    color: #000;
    margin: 5px;
}

.admin_current_user ul{
    margin: 0px;
    padding-left: 10px;
    list-style: none;
}

.rollo_admin_user{
    cursor: pointer;
    color: #2460e0;
}

#admin_info_user{
    position: absolute;
    left: 250px;
    top: 55px;
}

.pw_orphans{
    font-style: italic;
    color: #e00000;
}

.admin_title_name{
    font-weight: bold;
}

#pw_center{
    background-color: #fff;
}

#admin_index{
    padding: 5px;
}

#admin_tabs, #admin_tabs_providers, #pw_admin_groups_panel, #admin_tabs-titles_users{
    margin-top: 5px;
}

#admin_tabs_providers .pw_content, #admin_tabs_providers .pw_panel{
    margin-left: 0px;
}

/*admin providers*/

.admin_providers_form, #copyright{
    float: left;
    margin-right: 20px;
}

.admin_providers_form .field{
    margin-bottom: 10px;
    clear: both;

}

.admin_providers_form .field label{
    float: left;
    margin-right: 10px;
    line-height: 1.7em;
}

.group_cell_opts label{
    text-transform: uppercase;
}

#contacts #provider_contact .field, #contacts #provider_contact .sfield{
    float: left;
    margin-left: 10px;
}

.sfield{
    width: 50px;
    text-align: center;
    margin-top: -7px;

}

.pw_user_summary{
    margin-top: 10px;
    cursor: pointer;
    background-color: #e00000;
    color: #fff;
    padding: 3px;
    margin-top: 5px;
    float: left;
    -webkit-border-radius: 2px;
    -moz-border-radius: 2px;
    border-radius: 2px;
}

#admin_users_list li{
    float: left;
    margin: 10px 0 0 0;
    width: 200px;
}


.pw_display_inline{
    display: inline-block;
    position: relative;
    top: 3px;
}

#pw_admin_groups_panel .pw_panel-content{
    color: #000;
}

.pw_group_providers{
    color: #2460e0;
    font-weight: bold;
}

.pw_white{
    background-color: #fff;
}

